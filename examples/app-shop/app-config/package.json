{
  "name": "app-config",
  "version": "0.0.1",
  "description": "app-config",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "nodemon src/server/index.js --exec babel-node",
    "lint": "eslint .",
    "build": "NODE_ENV=production babel src/server -d dist/server && babel src/config -d dist/config",
    "watch": "NODE_ENV=production webpack -p --watch",
    "npm-check": "npm-check -u",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "test/*"
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.3",
    "lodash": "^4.17.4",
    "qs": "^6.5.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.17.3",
    "npm-check": "^5.6.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  }
}
